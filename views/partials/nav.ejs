  <% if (!user) { %>
    <link rel="stylesheet" href="/stylesheets/siwg.css">
  <% } %>
</head>
<body>
  <nav>
    <a href="/leads">🗣️</a>
    <a href="/profiles">🕴️</a>
    <% if (user) { %>
      <a href="/auth/logout">
        <button type="submit" class="btn btn-primary" id="logout-btn">Logout 👋</button>
      </a>
      <div class="dropdown">
        <button class="dropbtn">Browse</button>
        <div class="dropdown-content">
          <a href="/profiles">Profiles</a>
          <a href="/leads">Leads</a>
          <a href="/products">Products</a>
        </div>
      </div>
    <% } else { %>
      <a href="/auth/google" class="siwg-link">
        <button type="submit" class="siwg">
          <img id="siwg-image" alt="Sign in with Google" />
        </button>
      </a>
    <% } %>
  </nav>
