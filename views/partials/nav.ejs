  <% if (!user) { %>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="/stylesheets/siwg.css">
  <% } %>
</head>
<body>
  <nav>
    <% if (user) { %>
      <div class="dropdown">
        <button class="dropbtn"><i class="fa-solid fa-caret-down"></i> Browse</button>
        <div class="dropdown-content">
          <a href="/profiles">Profiles</a>
          <a href="/leads">All Leads</a>
          <a href="/products">Products</a>
        </div>
      </div>
      <a href="/auth/logout">
        <button type="submit" class="btn btn-primary" id="logout-btn"><i class="fa-solid fa-right-from-bracket"></i> Logout</button>
      </a>
    <% } else { %>
      <a href="/auth/google" class="siwg-link">
        <button type="submit" class="siwg">
          <img id="siwg-image" alt="Sign in with Google" />
        </button>
      </a>
    <% } %>
  </nav>
